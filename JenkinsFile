pipeline {

agent any

parameters {
choice(name: 'BRANCH_NAME',choices: ['Dev','Prod'], description: 'Select Branch Name')
choice(name: 'ENVIRONMENT_NAME', choices: ['Dev,'Prod'], description: 'Select enviornment name')
}

stages {
  stage ("Branch") {
    when {
      expression {
         params.BRANCH_NAME == 'Dev'
         }
         }
    steps {
           echo "Dev Branch is the selected again now"
           }
           }
           }
        } 
